<?xml version="1.0" encoding="UTF-8"?>
<PhoneColors>
        <!--PhoneColor.name：手机颜色的唯一标识，由项目、手软提供-->
        <!--ColorDescription：手机颜色描述，描述手机颜色-->
        <!--ColorDescriptionCN：手机颜色描述（中文），描述手机颜色-->
        <!--DefaultWallpaper :对应的锁屏壁纸-->
        <!--DefaultWallpaperLock :对应的桌面壁纸-->
        <!--ThemeDescriptionCN：主题描述（中文），描述主题名称-->
        <!--手机颜色唯一标识、手机颜色描述由项目或手软提供，对应主题由桌面主题组根据UI要求配置-->
        <!--该文件由OS产品中心桌面主题组维护-->

    <PhoneColor name="FF353330">
        <ColorDescription>Gray Shadow</ColorDescription>
        <ColorDescriptionCN>Gray Shadow</ColorDescriptionCN>
        <DefaultWallpaper>default_wallpaper_gray_shadow</DefaultWallpaper>
        <DefaultWallpaperLock>default_wallpaper_gray_shadow</DefaultWallpaperLock>
        <DefaultWallpaperComponent>N/A</DefaultWallpaperComponent>
    </PhoneColor>

    <PhoneColor name="FF90917F">
        <ColorDescription>Jade Fog</ColorDescription>
        <ColorDescriptionCN>Jade Fog</ColorDescriptionCN>
        <DefaultWallpaper>default_wallpaper_jade_fog</DefaultWallpaper>
        <DefaultWallpaperLock>default_wallpaper_jade_fog</DefaultWallpaperLock>
        <DefaultWallpaperComponent>N/A</DefaultWallpaperComponent>
    </PhoneColor>
</PhoneColors>
